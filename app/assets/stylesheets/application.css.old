@import "bootstrap";
@import url(http://fonts.googleapis.com/css?family=Kameron:400,700);
@import url(http://fonts.googleapis.com/css?family=Alegreya:400,400italic,700,700italic,900,900italic|Lusitana:400,700);
@import url(http://fonts.googleapis.com/css?family=Open+Sans:800,300);

html {

}
body {
  font-family: 'Lusitana', serif;
  font-size: 16px;
  letter-spacing: .75px;
  line-height: 32px;
  padding-top: 110px;
}
a, a:active, a:visited, a:focus {
	outline: none;
}
p {
	margin: 25px 0px;
	text-indent: 25px;
	text-align: justify;
}
textarea {
  resize: vertical;
}
.center {
  text-align: center;
  border-left: none;
}
h1 {
	text-transform: uppercase;
	letter-spacing: -.5px;
	font-family: 'Open Sans', sans-serif;
	margin: 40px 0px;
}
.navbar {
	height: 100px;
}
.navbar .nav > li {
    margin: 0 2px;
}
.navbar .brand {
	margin-left:40px;
}
.parallax-left {
	margin: 0px 0px;
	height: 2500px;
	position: relative;
	background-attachment: fixed;
}

.navbar .nav {
	font-family: 'Open Sans', sans-serif;
	font-weight: bold;
	text-transform: uppercase;
	font-size: 12px;
}
.path {
	position: absolute;
	background-attachment: fixed;
}
.target {
	background: transparent fixed;
	position: relative;
	left: 93px;
	z-index: 99;
}
#inflection-inner{
	position:relative;
}
